<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.tinglevpn.duckdns</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/etc/wireguard/duckdns-update.sh</string>
    </array>
    <key>StartInterval</key>
    <integer>300</integer>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/var/log/tinglevpn-duckdns.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/tinglevpn-duckdns.log</string>
</dict>
</plist>
